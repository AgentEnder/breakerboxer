{
  "version": 1,
  "projects": {
    "breakerboxer": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/breakerboxer",
      "sourceRoot": "apps/breakerboxer/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/breakerboxer",
            "index": "apps/breakerboxer/src/index.html",
            "main": "apps/breakerboxer/src/main.ts",
            "polyfills": "apps/breakerboxer/src/polyfills.ts",
            "tsConfig": "apps/breakerboxer/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/breakerboxer/src/favicon.ico", "apps/breakerboxer/src/assets"],
            "styles": ["apps/breakerboxer/src/styles.scss"],
            "scripts": [],
            "fileReplacements": [
              {
                "replace": "libs/xplat/core/src/lib/environments/environment.ts",
                "with": "apps/breakerboxer/src/environments/environment.ts"
              }
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "libs/xplat/core/src/lib/environments/environment.ts",
                  "with": "apps/breakerboxer/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "outputs": ["{options.outputPath}"]
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "breakerboxer:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "breakerboxer:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "breakerboxer:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/breakerboxer/src/test.ts",
            "polyfills": "apps/breakerboxer/src/polyfills.ts",
            "tsConfig": "apps/breakerboxer/tsconfig.spec.json",
            "karmaConfig": "apps/breakerboxer/karma.conf.js",
            "assets": ["apps/breakerboxer/src/favicon.ico", "apps/breakerboxer/src/assets"],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/breakerboxer/src/styles.scss"
            ],
            "scripts": []
          },
          "outputs": ["coverage/apps/breakerboxer"]
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/breakerboxer/tsconfig.app.json", "apps/breakerboxer/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {}
        }
      }
    },
    "breakerboxer-e2e": {
