{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "breakerboxer": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/breakerboxer",
      "sourceRoot": "apps/breakerboxer/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/breakerboxer",
            "index": "apps/breakerboxer/src/index.html",
            "main": "apps/breakerboxer/src/main.ts",
            "polyfills": "apps/breakerboxer/src/polyfills.ts",
            "tsConfig": "apps/breakerboxer/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/breakerboxer/src/favicon.ico",
              "apps/breakerboxer/src/assets"
            ],
            "styles": ["apps/breakerboxer/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/breakerboxer/src/environments/environment.ts",
                  "with": "apps/breakerboxer/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "breakerboxer:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "breakerboxer:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "breakerboxer:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/breakerboxer/src/test.ts",
            "polyfills": "apps/breakerboxer/src/polyfills.ts",
            "tsConfig": "apps/breakerboxer/tsconfig.spec.json",
            "karmaConfig": "apps/breakerboxer/karma.conf.js",
            "assets": [
              "apps/breakerboxer/src/favicon.ico",
              "apps/breakerboxer/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/breakerboxer/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/breakerboxer/tsconfig.app.json",
              "apps/breakerboxer/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {}
        }
      }
    },
    "breakerboxer-e2e": {
      "projectType": "application",
      "root": "apps/breakerboxer-e2e",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/breakerboxer-e2e/protractor.conf.js",
            "devServerTarget": "breakerboxer:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "breakerboxer:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/breakerboxer-e2e/tsconfig.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "utils": {
      "projectType": "library",
      "root": "libs/utils",
      "sourceRoot": "libs/utils/src",
      "prefix": "breakerboxer",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/utils/tsconfig.lib.json",
              "libs/utils/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/utils/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/utils/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      }
    },
    "layout": {
      "projectType": "library",
      "root": "libs/layout",
      "sourceRoot": "libs/layout/src",
      "prefix": "breakerboxer",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/layout/tsconfig.lib.json",
              "libs/layout/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/layout/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/layout/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      }
    }
  },
  "defaultProject": "breakerboxer",
  "cli": {
    "analytics": "48079284-5d9d-47d1-92a4-cb756e8f9b97",
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  }
}
